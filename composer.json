{
  "name": "quinluong/tracing-php",
  "type": "library",
  "description": "Distributed tracing wrapper for php",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^5.6||^7.0",
    "vantt/opentracing-php": "^1.0.0",
    "jukylin/jaeger-php": "v2.1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "<=6.5"
  },
  "archive": {
    "exclude": [
      "docker-compose.yml",
      "runTests.sh"
    ]
  },
  "autoload": {
    "psr-4": {
      "Tracing\\": "./src/Tracing"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tracing\\Tests\\": "./tests/Tracing"
    }
  }
}
